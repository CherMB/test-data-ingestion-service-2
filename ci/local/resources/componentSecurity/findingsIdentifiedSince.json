  },
  "queries": {
    "findingsIdentifiedCount": {
      "alias": "cb_security_findings",
      "query": {
        "size": 0,
        "query": {
          "bool": {
            "filter": [
              {
                "range": {
                  "first_identified_time": {
                    "gte": "{{.startDate}}",
                    "format": "yyyy-MM-dd",
                    "time_zone": "Asia/Calcutta"
                  }
                }
              },
              {
                "term": {
                  "org_id": "{{.orgId}}"
                }
              },
              {
                "term": {
                  "profile_group_type": "component_branch"
                }
              }
            ],
            "must_not": {
              "terms": {
                "category": ["BUG", "CODE_SMELL", "HOTSPOT", "SECURITY_HOTSPOT"]
              }
            }
          }
        },
        "aggs": {
          "remediation_status": {
            "terms": {
              "field": "remediation_status",
              "size": 10
            },
            "aggs": {
              "tracking_id": {
                "terms": {
                  "script": "doc['finding_code'].value + '_' + doc['tracking_id'].value",
                  "size": 10000
                }
              }
            }
          }
        }
      }
    }
  }
}
